    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "@vercel/ncc": "^0.38.3",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "ts-node": "^10.9.2",
    "husky": "^9.1.7",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0",
    "dotenv": "^17.0.0"
  }
